        "\n",
        "import os\n",
        "from roboflow import Roboflow\n",
        "import tensorflow as tf\n",
        "import cv2\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Initialize Roboflow\n",
       
        "rf = Roboflow(api_key=os.environ.get(\"ROBOFLOW_API_KEY\"))\n",
        
        "project = rf.workspace(\"my-workspace\").project(\"colony-counter-0ltmm\")\n",
        "dataset = project.version(2).download(\"tfrecord\")\n",
        "\n",
        "# Define model architecture\n",
        "def create_model():\n",
        "    model = tf.keras.Sequential([\n",
        "        tf.keras.layers.Conv2D(32, 3, activation='relu', input_shape=(224, 224, 3)),\n",
        "        tf.keras.layers.MaxPooling2D(),\n",
        "        tf.keras.layers.Conv2D(64, 3, activation='relu'),\n",
        "        tf.keras.layers.MaxPooling2D(),\n",
        "        tf.keras.layers.Conv2D(64, 3, activation='relu'),\n",
        "        tf.keras.layers.Flatten(),\n",
        "        tf.keras.layers.Dense(64, activation='relu'),\n",
        "        tf.keras.layers.Dense(1)  # Output layer for colony count\n",
        "    ])\n",
        "    return model\n",
        "\n",
        "# Data preprocessing\n",
        "def preprocess_image(image_path):\n",
        "    img = cv2.imread(image_path)\n",
        "    img = cv2.resize(img, (224, 224))\n",
        "    img = img / 255.0\n",
        "    return img\n",
        "\n",
        "# Train model\n",
        "def train_model(model, train_data, validation_data):\n",
        "    model.compile(optimizer='adam',\n",
        "                 loss='mse',\n",
        "                 metrics=['mae'])\n",
        "\n",
        "    history = model.fit(\n",
        "        train_data,\n",
        "        validation_data=validation_data,\n",
        "        epochs=10\n",
        "    )\n",
        "    return history\n",
        "\n",
        "def main():\n",
        "    # Create and train model\n",
        "    model = create_model()\n",
        "\n",
        "    
        "\n",
        "    print(\"Model created and ready for training\")\n",
        "\n",
        "    # For inference on new images:\n",
        "    def predict_colonies(image_path):\n",
        "        img = preprocess_image(image_path)\n",
        "        prediction = model.predict(np.expand_dims(img, axis=0))\n",
        "        return int(prediction[0][0])\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    main()\n"
      ]
